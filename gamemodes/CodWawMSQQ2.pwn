				TextDrawHideForPlayer(playerid, Capture_PTD[i]);
    			break;
			}
		}
		new bool:notempty;
		foreach(new i : Player)
		{
			if(GetPlayerVehicleID(i) == vehid)
			{
				if(GetPlayerState(i) == PLAYER_STATE_DRIVER)
				{
					notempty = true;
					break;
				}
			}
		}
		if(!notempty) RemovePlayerFromVehicle(playerid);
