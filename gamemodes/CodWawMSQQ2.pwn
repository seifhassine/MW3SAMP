	PlayerTextDrawSetString(playerid, SpecPanel_PTD[playerid][1], st);

	st[0] = EOS; //Let's assign the index 0 to EOS - End of String, which will clear all it's characters.

	new WeapName[17], slot, weap, ammo, wh;

	for (slot = 0; slot < 14; slot++)
	{
		GetPlayerWeaponData(id, slot, weap, ammo);
		if(ammo != 0)
		{
			if(weap != 0)
			{
			    if(slot == 0) ammo = 1;
				GetWeaponName(weap, WeapName, sizeof(WeapName) );
				if(weap == 18) WeapName = "Molotov";
				format(st, sizeof(st), "%s~g~Slot %d: %s (%d)~n~", st, slot, WeapName, ammo);
