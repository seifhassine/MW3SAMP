forward GetCTTags(playerid);
public GetCTTags(playerid)
{
    new cc = cache_num_rows(), maxteams = CLAN_TOURNAMENT[CT_INITIAL_TEAMS];
	if(cc < maxteams)
	{
		SCM(playerid, Dred, "ERROR: You have a wrong clan ID, clan tournament has been canceled."); CLAN_TOURNAMENT[CT_ON] = false;
		foreach(new i : CT_TEAMS) {CT_TEAM[i] = -1;}
		Iter_Clear(CT_TEAMS);
		return 1;
	}
	new idd[8],gottag[8][6];
	for(new i =0; i < cc; i++)
	{
		cache_get_value_name(i,"ctag",gottag[i],6);
		cache_get_value_name_int(i,"cid", idd[i]);
	}
	for(new i=0; i < maxteams; i++)
	{
	    for(new j=0; j < maxteams; j++)
	    {
	    	if(CT_TEAM[i] == idd[j])
	    	{
	    	    format(CT_TEAM_TAG[i], 6, "%s", gottag[j]);
	    	}
	    }
	}
	new strr[342];
	new maxmembers = CLAN_TOURNAMENT[CT_MAX];
	new query[450];
    new str[20];
    for(new i=0; i < 4; i++) format(CT_SEMI_TAG[i], 7, "?");
    format(CT_FINALS_TAG[0], 7, "?");
    format(CT_FINALS_TAG[1], 7, "?");
	if(maxteams == 4)
	{
		format(strr, sizeof strr, "You have successfully created a clan tournament\n\nDetails:\n\n\nMax Teams:%d\nMax Team Members:%d\n\nTeams:\n >%s\n>%s\n>%s\n>%s", maxteams, maxmembers, CT_TEAM_TAG[0],CT_TEAM_TAG[1],CT_TEAM_TAG[2],CT_TEAM_TAG[3]);
        CLAN_TOURNAMENT[CT_SEMI_TEAM1][0] = CT_TEAM[0];
