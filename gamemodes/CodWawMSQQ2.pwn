	if(PlayerInfo[playerid][Streamer] == 1) format(string,sizeof(string),"{FF3778}[STREAMER]%s%s[%d]: %s",string,playername,playerid,chat);
	else if(vrk < 1) format(string,sizeof(string),"%s%s[%d]: {FFFFFF}%s",string,playername,playerid,chat);
	else if(vrk == 6) format(string,sizeof(string),"{FFFF00}[SPONSOR]%s%s[%d]: %s",string,playername,playerid,chat);
	else format(string,sizeof(string),"{FFFF00}[VIP]%s%s[%d]: %s",string,playername,playerid,chat);
	foreach(new i : Player)
	{
		if(IsSpectator[i]) continue;
		SCM(i,-1,string);
	}
	return 0;
}

