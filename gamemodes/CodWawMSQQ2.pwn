forward CheatCheck();
public CheatCheck()
{
	foreach(new i : Player)
	{
		if(PlayerInfo[i][Rage] != 0) PlayerInfo[i][Rage] --;
		if(PlayerInfo[i][Dead] == 0)
		{
			if(GetPlayerCameraMode(i) == 53)
			{
				new Float:kLibPos[3];
				GetPlayerCameraPos(i, kLibPos[0], kLibPos[1], kLibPos[2]);
				if ( kLibPos[2] < -50000.0 || kLibPos[2] > 50000.0 )
				{
					BanEx(i, "WeaponCrasher");
					return 0;
				}
			}
		}
		if(GetPlayerSpecialAction(i) == SPECIAL_ACTION_USEJETPACK)
		{
			if(gClass[i] != JET)
			{
				if(PlayerInfo[i][Level] < 1)
				{
				    new str[128];
					format(str, sizeof(str), "{00FF00}<Anti-Cheat>: {FFFFFF}%s(%d) has been kicked for jetpack hacks.", PlayerInfo[i][Nick], i);
	 				AntiCheatNotification(-1, str);
					KickX(i);
				}
			}
		}
		if(GetPlayerMoney(i) > PlayerInfo[i][Cash])
		{
			ResetPlayerMoney(i);
			GivePlayerMoney(i,PlayerInfo[i][Cash]);
		}
	}
	return 1;
}

