CMD:pm(playerid, params[])
{
	if(PlayerInfo[playerid][Muted]) return SCM(playerid,Dred,"* You are muted");
	new str[128], str2[128], id, adminstr[128];
	if(sscanf(params,"ds[128]", id, str2)) return SendClientMessage(playerid, red,"USAGE: /pm [id] [message]");
	new remain = GetTickCount() - PlayerInfo[playerid][PMS];
	if(remain < 2000)
	{

		Form(playerid,Dred,"* Please wait (%d) Seconds before using this command again", (2000-remain)/1000);
		return 1;
	}
	if(!IsPlayerConnected(id)) return SendClientMessage(playerid, red,"Player is not connected");
	if(id == playerid) return SendClientMessage(playerid, red,"You cannot PM yourself");
	if(Bit_Get(ToggleState[id], TOGGLE_DND)) return SendClientMessage(playerid, red,"That player is in do not disturb mode!");
	if(IsAdvertisement(params))
	{
		new string[68];
		format(string, 68, "%s has been kicked for advertising a server in pm.", PlayerInfo[playerid][Nick]);
		SendClientMessageToAll(COLOR_YELLOW, string);
		Kick(playerid);
		return 0;
	}
	if(IsBlocked(id, playerid)) return ERR(playerid, "Cannot send pm, this user used /block on you.");
	pms[id][0] = pms[id][1];
	pms[id][1] = pms[id][2];
	pms[id][2] = pms[id][3];
	pms[id][3] = pms[id][4];
	new nam[24], nam2[24]; format(nam, 24, PlayerInfo[playerid][Nick]); format(nam2, 24, PlayerInfo[id][Nick]);
	format(pms[id][4], 144, "%s:%s", nam, str2);
	BadWord(str2);
	format(str, sizeof(str),"PM to [%d]%s: %s", id, nam2, str2);
	SendClientMessage(playerid, yellow, str);
	format(str, sizeof(str),"PM from [%d]%s: %s", playerid, nam, str2);
	SendClientMessage(id, yellow, str);
	SendClientMessage(id, -1,"Use "cblue"/rpm [message] "cwhite"to reply to this PM");
	format(adminstr, sizeof(adminstr),"PM from %s[%d] to %s[%d]: %s", nam, playerid, nam2, id, str2);
	if(ServerInfo[ReadPMs]) MessageTo4(grey, adminstr);
	LastPm[id] = playerid;
	PlayerInfo[playerid][PMS] = GetTickCount();
	return 1;
}
CMD:block(playerid, params[])
{
	new id;
	if(sscanf(params,"d", id)) return SendClientMessage(playerid, red,"USAGE: /block [id]");
	if(!IsPlayerConnected(id)) return SendClientMessage(playerid, red,"Player is not connected");
	if(id == playerid) return SendClientMessage(playerid, red,"You cannot block yourself");
	if(IsBlocked(playerid, id))
	{
		SCM(playerid, COLOR_YELLOW, "User unblocked");
		Iter_Remove(BLOCKED[playerid], id);
	}
	else
	{
		SCM(playerid, COLOR_YELLOW, "User blocked (Use /block to unblock them).");
		Iter_Add(BLOCKED[playerid], id);
	}
	return 1;
}
