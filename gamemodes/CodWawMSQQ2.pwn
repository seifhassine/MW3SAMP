stock NukeKill(playerid,id)
{
	new msg[156];
	SetPlayerHealth(id,0);
	GameTextForPlayer(id,"~r~Nuked",3000,3);
	GiveCash(playerid,1500);
	SetPlayerScore(playerid,GetPlayerScore(playerid)+1);
	if(cPerk[id][P_DEATH] == 0)
	{
        new nam[24]; format(nam, 24, PlayerInfo[playerid][Nick]);
		format(msg, sizeof(msg), "{FF8000}[DEATH] {FFFFFF}You got killed by {FF8000}%s(%d)' Nuclear bomb{FFFFFF} You lost -2500$",nam,playerid);
		GiveCash(id,-2500);
	}else SCM(id, COLOR_YELLOW,"[Clan Perk] You did not lose cash after your death.");
	SendClientMessage(id,-1,msg);
	SendDeathMessage(playerid,id,53);
	new Float:x,Float:y,Float:z;
	GetPlayerPos(id,x,y,z);
	PlayerPlaySound(id,1159,x,y,z);
	CreateExplosion(x,y,z,0,1);
	PlayerInfo[playerid][Kills] ++;
	Streak[playerid] ++;
}
