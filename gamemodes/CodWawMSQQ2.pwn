	    mysql_format(mysql, Query, sizeof Query, "CREATE EVENT `'%d'_viptemp` ON SCHEDULE AT CURRENT_TIMESTAMP() + INTERVAL 30 DAY DO UPDATE `playerbase` SET `vip`=3 WHERE `id`='%d'",dbid,dbid);
