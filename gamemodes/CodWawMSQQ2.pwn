			if(i != playerid)
			{
				if(GetPlayerTeam(i) != tt)
				{
				    if(GetPlayerState(i) != PLAYER_STATE_SPECTATING)
				    {
						if(!PlayerInfo[i][OnDuty])
						{
						    if(x != -1) if(PlayerInfo[i][InClan] == x) continue;
