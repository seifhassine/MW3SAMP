{
	Inviter[wish] = INVALID_PLAYER_ID;
	Invited[playerid] = INVALID_PLAYER_ID;
	SCM(playerid,-1,"Friend request canceled");
	SCM(wish,-1,"Friend request canceled");
	return 1;
}

