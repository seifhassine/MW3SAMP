CMD:cperks(playerid,params[])
{
	if(PlayerInfo[playerid][InClan] == -1) return ERR(playerid,"You are not in any clan.");
	new str[80*MAX_PERKS], Day, Hour, Minute, Second, remain;
	str = "Perk\tRemaining Time\n";
	new bool:hh;
	hh = false;
	for(new i; i < MAX_PERKS; i++)
	{
		if(cPerk[playerid][i] > 0)
		{
			hh = true;
			new bla[87];
			remain = cPerk[playerid][i]+ 432000 - gettime(); //86400 1 day in second
			if(remain < 0) continue;
			Day = (remain - (remain % 86400)) / 86400;
			remain = remain % 86400;
			Hour = (remain - (remain % 3600)) / 3600;
			remain = remain % 3600;
			Minute = (remain - (remain % 60)) / 60;
			Second = remain % 60;
			switch(i)
			{
				case 0: format(bla, sizeof bla, "Sniper on Spawn\t %d Days %d Hours %d Minutes %d Seconds\n", Day, Hour, Minute, Second);
				case 1: format(bla, sizeof bla, "Armour on Spawn\t %d Days %d Hours %d Minutes %d Seconds\n", Day, Hour, Minute, Second);
				case 2: format(bla, sizeof bla, "Fast Capture\t %d Days %d Hours %d Minutes %d Seconds\n", Day, Hour, Minute, Second);
				case 3: format(bla, sizeof bla, "No Death Tax\t %d Days %d Hours %d Minutes %d Seconds\n", Day, Hour, Minute, Second);
				case 4: format(bla, sizeof bla, "Double Cash on Capping-Killing\t %d Days %d Hours %d Minutes %d Seconds\n", Day, Hour, Minute, Second);
				case 5: format(bla, sizeof bla, "\nHelmet on Spawn\t %d Days %d Hours %d Minutes %d Seconds\n", Day, Hour, Minute, Second);
				case 6: format(bla, sizeof bla, "Gasmask on Spawn\t %d Days %d Hours %d Minutes %d Seconds\n", Day, Hour, Minute, Second);
				case 7: format(bla, sizeof bla, "Clan Base\t %d Days %d Hours %d Minutes %d Seconds", Day, Hour, Minute, Second);
			}
			strcat(str, bla);
		}
	}
	if(hh) ShowPlayerDialogEx(playerid, DIALOG_CPERKS, DIALOG_STYLE_TABLIST_HEADERS, "Clan Perks", str, "ok", "");
	else ERR(playerid, "Your clan does not have any perks activated.");
	return 1;
}
