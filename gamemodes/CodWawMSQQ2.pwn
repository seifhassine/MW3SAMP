public OnPlayerRequestSpawn(playerid)
{
	if(PlayerInfo[playerid][LoggedIn] == false)
	{
		SCM(playerid,Dred,"Please login or register.");
		return 0;
	}
	new diff, kk;
	for(new i = 1; i < MAX_TEAMS; i++)
	{
	    if(i == TEAM_NONE) continue;
		kk = GetTeamCount(gTeam[playerid]) - GetTeamCount(i);
		if(kk > diff) diff = kk;
	}
	if(PlayerInfo[playerid][dRank] < 3)
	{
		if(diff > 20)
		{
			SCM(playerid,red,"ERROR: This team is full. Try a different team!");
			return 0;
		}
	}
	return 1;
}
