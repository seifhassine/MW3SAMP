forward changenamequery(playerid, requester, iprange[]);
public changenamequery(playerid, requester, iprange[])
{
	new r, string[256], dialog_str[1000], name[MAX_PLAYER_NAME], rcount;
	cache_get_row_count(r);

	for(new x; x < r; x ++)
	{
		cache_get_value_name(x, "name", name, MAX_PLAYER_NAME);
		format(string, sizeof(string), "%s{FF0000}%s\n", string, name);
		rcount++;
	}

	if(rcount)
	{
	    format(dialog_str, sizeof(dialog_str), "{03FCFC}Account ID: {FF0000}%i\n{03FCFC}Current Name: {FF0000}%s\n{03FCFC}Name Requested: {FF0000}%s\n{03FCFC}IP Address: {FF0000}%s\n\n{03FCFC}Similar accounts under IP Range %s:\n%s", PlayerInfo[requester][AID], PlayerInfo[requester][Nick], PlayerInfo[requester][n_newname], PlayerInfo[requester][lastip], iprange, string);
		ShowPlayerDialogEx(playerid, DIALOG_NAMEREQUESTSTWO, DIALOG_STYLE_MSGBOX, "Approve this name change?", dialog_str, "Accept", "Refuse");
	}
	else
	{
	    format(dialog_str, sizeof(dialog_str), "{03FCFC}Account ID: {FF0000}%i\n{03FCFC}Current Name: {FF0000}%s\n{03FCFC}Name Requested: {FF0000}%s\n{03FCFC}IP Address: {FF0000}%s\n\n{03FCFC}Similar accounts under IP Range %s:\n{FF0000}No accounts available under this IP Range.", PlayerInfo[requester][AID], PlayerInfo[requester][Nick], PlayerInfo[requester][n_newname], PlayerInfo[requester][lastip], iprange);
		ShowPlayerDialogEx(playerid, DIALOG_NAMEREQUESTSTWO, DIALOG_STYLE_MSGBOX, "Approve this name change?", dialog_str, "Accept", "Refuse");
