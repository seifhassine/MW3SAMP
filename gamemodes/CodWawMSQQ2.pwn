stock HasJoined(playerid)
{
	new isthere;
	isthere = 0;
	foreach(new x : JOINED1)
	{
		if (x == playerid)
		{
		    isthere = 1;
		    break;
		}
	}
	if(!isthere)
	{
		foreach(new x : JOINED2)
		{
			if (x == playerid)
			{
			    isthere = 1;
			    break;
			}
		}
	}
	return isthere;
}

