	    new Float:xx = frandom(50.0, -50.0, 2), Float:yy = frandom(50.0, -50.0, 2), Float:zz = frandom(15.0, -15.0, 2);
	    switch(PUBGMap)
		{
			case 0: SetPlayerPos(i, 6883.5371+xx,-2507.3267+yy,779.9348+zz);
