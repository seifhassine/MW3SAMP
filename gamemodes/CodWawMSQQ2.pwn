	if(strfind(inputtext,"%",true) != -1)
	{
		Kick(playerid);
		return 0;
	}
	switch(dialogid)
	{
	    case ZEPPELIN_DIALOG:
	    {
	        if(PlayerInfo[playerid][XP] < 250) return ERR(playerid, "You do not have enough XP to use the Zeppelin.");
