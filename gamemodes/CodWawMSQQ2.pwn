const PLAYER_SYNC = 207;
IPacket:PLAYER_SYNC(playerid, BitStream:bs)
{
    new onFootData[PR_OnFootSync];

    BS_IgnoreBits(bs, 8);
    BS_ReadOnFootSync(bs, onFootData);

    if(IsNan(onFootData[PR_position][0]) ||
    	IsNan(onFootData[PR_position][1]) ||
    	IsNan(onFootData[PR_position][2]) ||
    	IsNan(onFootData[PR_quaternion][0]) ||
    	IsNan(onFootData[PR_quaternion][1]) ||
    	IsNan(onFootData[PR_quaternion][2]) ||
    	IsNan(onFootData[PR_velocity][0]) ||
    	IsNan(onFootData[PR_velocity][1]) ||
    	IsNan(onFootData[PR_velocity][2]) ||
    	IsNan(onFootData[PR_surfingOffsets][0]) ||
    	IsNan(onFootData[PR_surfingOffsets][1]) ||
