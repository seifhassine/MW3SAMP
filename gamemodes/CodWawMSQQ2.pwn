		if(PlayerInfo[i][CMuted])
		{
			if(-- PlayerInfo[i][CMuteTime] < 1)
			{
				SCM(i,green,"UNMUTE: You have been clan unmuted");
				PlayerInfo[i][CMuted] = false;
			}
		}
		if(KillTDShow[i]) if(GetTickCount() - KillTDTick[i] > 0) HideKillsTD(i);
	}
	return 1;
}

