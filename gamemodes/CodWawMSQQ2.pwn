					    case 31: 	{ type = LOOT_M4;    		tag = "M16A4"; 					}
					    case 33: 	{ type = LOOT_RIFLE; 		tag = "KAR98K"; 				}
					    case 16: 	{ type = LOOT_GRENADE; 		tag = "Grenade"; 				}
