stock SendTeamMessage(teamid, color, string[])
{
	foreach(Player,x) { if(gTeam[x] == teamid) SendClientMessage(x, color, string); }
	return 1;
}
stock GiveTeamScore(teamid, amount)
{
	foreach(Player,x) { if(gTeam[x] == teamid) SetPlayerScore(x, GetPlayerScore(x)+amount); }
	return 1;
}
